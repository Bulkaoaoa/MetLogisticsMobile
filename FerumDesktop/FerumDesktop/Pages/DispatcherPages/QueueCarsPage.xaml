<Page x:Class="FerumDesktop.Pages.DispatcherPages.QueueCarsPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:FerumDesktop.Pages.DispatcherPages"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="QueueCarsPage">

    <Grid>
        <Border Background="{StaticResource MainBlue}" Height="250" Width="180">
            <Grid>
                <StackPanel Margin="10">
                    <Image Height="50" Width="50" Source="/FerumDesktop;component/Assets/Icons.images/IconsHighQual/IconCrane.png">
                    </Image>
                    <TextBlock Style="{StaticResource StyleTbkMain}" Margin="5" Text="{Binding Warehouse}"></TextBlock>
                </StackPanel>
            </Grid>
        </Border>
    </Grid>
</Page>
